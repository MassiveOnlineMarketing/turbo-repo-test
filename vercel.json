{
  "version": 2,
  "builds": [
    {
      "src": "apps/app-router/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/pages-router/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/syntax-ts/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(?!docs|app).*",
      "dest": "/apps/app-router/*"
    },
    {
      "src": "/app/(.*)",
      "dest": "/apps/pages-router/$1"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/apps/syntax-ts/$1"
    }
  ],
  "env": {
    "PNPM_VERSION": "8.0.0"
  }
}